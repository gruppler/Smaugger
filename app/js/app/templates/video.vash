<td class="buttons">
  <button class="delete">x</button>
</td>
<td class="info">
  <div class="name">@model.name</div>
  <div class="info">
    <span class="duration">@model.duration</span>
    <span class="size">@model.size</span>
  </div>
</td>
<td class="video">
  <video>
    <source src="@model.path" type="@model.type"/>
  </video>
</td>
